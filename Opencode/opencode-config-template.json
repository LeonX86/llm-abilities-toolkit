{
  "models": { // 模型配置对象，包含所有可用的 AI 模型定义
    "type": "object", // 定义 models 字段为对象类型
    "propertyNames": { // 定义模型对象的属性名（模型 ID）的验证规则
      "type": "string" // 模型 ID 必须是字符串类型，如 "gpt-4", "claude-3-opus" 等
    },
    "additionalProperties": { // 定义每个模型的配置结构，键为模型 ID，值为模型配置对象
      "type": "object", // 每个模型的配置必须是对象类型
      "properties": { // 模型配置对象包含的所有可能属性
        "id": { // 模型的唯一标识符（可选，通常对象的键名就是模型 ID）
          "type": "string" // ID 必须是字符串，如 "gpt-4-turbo", "claude-3-5-sonnet-20241022"
        },
        "name": { // 模型的显示名称
          "type": "string" // 名称必须是字符串，用于在 UI 中显示，如 "GPT-4 Turbo", "Claude 3.5 Sonnet"
        },
        "family": { // 模型所属的系列或家族
          "type": "string" // 系列名称必须是字符串，如 "gpt-4", "claude-3", "gemini" 等
        },
        "release_date": { // 模型的发布日期
          "type": "string" // 日期必须是字符串格式，建议使用 ISO 8601 格式如 "2024-01-15"
        },
        "attachment": { // 模型是否支持附件功能（如上传文件）
          "type": "boolean" // 布尔值：true 表示支持附件，false 表示不支持
        },
        "reasoning": { // 模型是否支持推理功能（如思维链、分步推理等）
          "type": "boolean" // 布尔值：true 表示支持推理模式，false 表示不支持
        },
        "temperature": { // 模型是否支持 temperature 参数（控制输出随机性）
          "type": "boolean" // 布尔值：true 表示可调节 temperature，false 表示不支持
        },
        "tool_call": { // 模型是否支持工具调用/函数调用功能
          "type": "boolean" // 布尔值：true 表示支持工具调用，false 表示不支持
        },
        "interleaved": { // 模型是否支持交错内容（如文本和图片混合）
          "anyOf": [ // 该字段可以是以下任意一种类型
            { // 第一种类型：简单的布尔值
              "type": "boolean", // 布尔类型
              "const": true // 只能是 true，表示支持交错内容
            },
            { // 第二种类型：对象配置，用于指定交错内容的具体字段
              "type": "object", // 对象类型
              "properties": { // 交错配置对象的属性
                "field": { // 指定哪个字段包含交错内容
                  "type": "string", // 字段名必须是字符串
                  "enum": [ // 只能是以下枚举值之一
                    "reasoning_content", // 推理内容字段
                    "reasoning_details" // 推理详情字段
                  ]
                }
              },
              "required": [ // 必填字段
                "field" // 如果使用对象形式，field 字段是必需的
              ],
              "additionalProperties": false // 不允许有其他额外属性
            }
          ]
        },
        "cost": { // 模型的价格配置
          "type": "object", // 价格配置必须是对象类型
          "properties": { // 价格对象包含的所有可能属性
            "input": { // 输入 token 的价格
              "type": "number" // 必须是数字，通常以美元/百万 tokens 为单位，如 3.0 表示 $3/1M tokens
            },
            "output": { // 输出 token 的价格
              "type": "number" // 必须是数字，通常以美元/百万 tokens 为单位，如 15.0 表示 $15/1M tokens
            },
            "cache_read": { // 缓存读取的价格
              "type": "number" // 必须是数字，从缓存读取 token 的价格（通常比常规输入便宜）
            },
            "cache_write": { // 缓存写入的价格
              "type": "number" // 必须是数字，写入缓存的价格（通常比常规输入稍贵）
            },
            "context_over_200k": { // 当上下文超过 20 万 tokens 时的特殊价格
              "type": "object", // 超长上下文价格配置必须是对象类型
              "properties": { // 超长上下文价格对象的属性
                "input": { // 超长上下文的输入 token 价格
                  "type": "number" // 必须是数字，通常比标准价格更高
                },
                "output": { // 超长上下文的输出 token 价格
                  "type": "number" // 必须是数字，通常比标准价格更高
                },
                "cache_read": { // 超长上下文的缓存读取价格
                  "type": "number" // 必须是数字
                },
                "cache_write": { // 超长上下文的缓存写入价格
                  "type": "number" // 必须是数字
                }
              },
              "required": [ // 超长上下文配置中的必填字段
                "input", // 输入价格必填
                "output" // 输出价格必填
              ],
              "additionalProperties": false // 不允许有其他额外属性
            }
          },
          "required": [ // 价格配置中的必填字段
            "input", // 输入价格必填
            "output" // 输出价格必填
          ],
          "additionalProperties": false // 不允许有其他额外属性
        },
        "limit": { // 模型的限制配置
          "type": "object", // 限制配置必须是对象类型
          "properties": { // 限制对象包含的所有可能属性
            "context": { // 上下文窗口大小限制
              "type": "number" // 必须是数字，表示最大上下文 tokens 数，如 200000 (200k), 1048576 (1M) 等
            },
            "input": { // 输入 tokens 的最大限制
              "type": "number" // 必须是数字，表示单次请求允许的最大输入 tokens 数
            },
            "output": { // 输出 tokens 的最大限制
              "type": "number" // 必须是数字，表示单次响应允许的最大输出 tokens 数，如 64000, 65535, 65536 等
            }
          },
          "required": [ // 限制配置中的必填字段
            "context", // 上下文限制必填
            "output" // 输出限制必填
          ],
          "additionalProperties": false // 不允许有其他额外属性
        },
        "modalities": { // 模型支持的模态（输入输出类型）
          "type": "object", // 模态配置必须是对象类型
          "properties": { // 模态对象包含的属性
            "input": { // 模型支持的输入模态列表
              "type": "array", // 输入模态必须是数组类型
              "items": { // 数组中每个元素的类型
                "type": "string", // 每个模态必须是字符串
                "enum": [ // 只能是以下枚举值之一
                  "text", // 文本输入
                  "audio", // 音频输入
                  "image", // 图片输入
                  "video", // 视频输入
                  "pdf" // PDF 文档输入
                ]
              }
            },
            "output": { // 模型支持的输出模态列表
              "type": "array", // 输出模态必须是数组类型
              "items": { // 数组中每个元素的类型
                "type": "string", // 每个模态必须是字符串
                "enum": [ // 只能是以下枚举值之一
                  "text", // 文本输出
                  "audio", // 音频输出
                  "image", // 图片输出
                  "video", // 视频输出
                  "pdf" // PDF 文档输出
                ]
              }
            }
          },
          "required": [ // 模态配置中的必填字段
            "input", // 输入模态列表必填
            "output" // 输出模态列表必填
          ],
          "additionalProperties": false // 不允许有其他额外属性
        },
        "experimental": { // 模型是否为实验性功能
          "type": "boolean" // 布尔值：true 表示实验性模型，false 表示正式版本
        },
        "status": { // 模型的当前状态
          "type": "string", // 状态必须是字符串
          "enum": [ // 只能是以下枚举值之一
            "alpha", // Alpha 阶段，早期测试版本
            "beta", // Beta 阶段，公开测试版本
            "deprecated" // 已弃用，不建议使用
          ]
        },
        "options": { // 模型的自定义选项配置
          "type": "object", // 选项配置必须是对象类型
          "propertyNames": { // 选项对象的属性名验证规则
            "type": "string" // 选项名必须是字符串，如 "max_tokens", "top_p" 等
          },
          "additionalProperties": {} // 允许任意类型的额外属性值，用于存储模型特定的配置参数
        },
        "headers": { // 请求该模型时需要附加的 HTTP 头
          "type": "object", // 头部配置必须是对象类型
          "propertyNames": { // 头部名称验证规则
            "type": "string" // 头部名称必须是字符串，如 "Authorization", "X-API-Key" 等
          },
          "additionalProperties": { // 头部值的类型
            "type": "string" // 头部值必须是字符串，如 "Bearer token", "api_key_value" 等
          }
        },
        "provider": { // 模型提供商的配置
          "type": "object", // 提供商配置必须是对象类型
          "properties": { // 提供商对象包含的属性
            "npm": { // NPM 包名，用于标识提供商的 SDK
              "type": "string" // NPM 包名必须是字符串，如 "@anthropic-ai/sdk", "openai" 等
            }
          },
          "required": [ // 提供商配置中的必填字段
            "npm" // NPM 包名必填
          ],
          "additionalProperties": false // 不允许有其他额外属性
        },
        "variants": { // 模型的变体配置，用于同一模型的不同版本或配置
          "description": "Variant-specific configuration", // 字段说明：变体特定配置
          "type": "object", // 变体配置必须是对象类型
          "propertyNames": { // 变体名称验证规则
            "type": "string" // 变体名称必须是字符串，如 "low", "high", "medium", "minimal", "max" 等
          },
          "additionalProperties": { // 每个变体的配置
            "type": "object", // 变体配置必须是对象类型
            "properties": { // 变体配置对象的属性
              "disabled": { // 是否禁用该模型的此变体
                "description": "Disable this variant for the model", // 字段说明：为该模型禁用此变体
                "type": "boolean" // 布尔值：true 表示禁用该变体，false 或未设置表示启用
              }
            },
            "additionalProperties": {} // 允许任意类型的额外属性，用于存储变体特定的配置（如 thinkingLevel、thinkingConfig 等）
          }
        }
      },
      "additionalProperties": false // 不允许模型配置对象有其他未定义的属性
    }
  },
  "whitelist": { // 白名单配置，指定允许使用的模型列表
    "type": "array", // 白名单必须是数组类型
    "items": { // 数组中每个元素的类型
      "type": "string" // 每个模型 ID 必须是字符串，只有在白名单中的模型才能被使用
    }
  },
  "blacklist": { // 黑名单配置，指定禁止使用的模型列表
    "type": "array", // 黑名单必须是数组类型
    "items": { // 数组中每个元素的类型
      "type": "string" // 每个模型 ID 必须是字符串，黑名单中的模型将被排除，即使在白名单中
    }
  }
}